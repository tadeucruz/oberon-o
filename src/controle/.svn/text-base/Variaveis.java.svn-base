package controle;

import java.util.ArrayList;

public class Variaveis {

	private boolean mem = true;
	private ArrayList<String> reg = new ArrayList<String>();
	private String v;

	public String getV() {
		return v;
	}

	public void setV(String v) {
		this.v = v;
	}

	public boolean isMem() {
		return mem;
	}

	public void setMem(boolean mem) {
		this.mem = mem;
	}

	public ArrayList<String> getReg() {
		return reg;
	}

	public void setReg(String rega) {
		reg.add(rega);
	}

	public boolean reg_empty() {
		return reg.isEmpty();
	}

	public boolean getMem() {
		return mem;
	}

	@Override
	public String toString() {
		return "Variaveis [v=" + v + "]";
	}

}
