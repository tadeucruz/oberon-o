package controle;

public class ControleDead {

	private String var;
	private String info;

	public String getVar() {
		return var;
	}

	public void setVar(String var) {
		this.var = var;
	}

	public String getInfo() {
		return info;
	}

	public void setInfo(String info) {
		this.info = info;
	}

	@Override
	public boolean equals(Object obj) {
		ControleDead cd = (ControleDead) obj;
		return var.equals(cd.getVar());
	}
}
